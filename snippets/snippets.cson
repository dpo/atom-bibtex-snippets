  'Unpublished':
    'prefix': 'unpublished'
    'body': """
      @Unpublished{${1:author}-${2:year},
        author = {$1},
        title = {$3},
        note = {$4},
        OPTkey = {},
        OPTmonth = {},
        OPTyear = {$2},
        OPTannote = {}
      }
    """

  'Preamble':
    'prefix': 'preamble'
    'body': '@Preamble{{${1:message}}}'
  'Proceedings':
    'prefix': 'proceedings'
    'body': """
      @Proceedings{${1:editor}-${2:year},
        title = {},
        year = {$2},
        OPTkey = {},
        OPTbooktitle = {},
        OPTeditor = {$1},
        OPTvolume = {},
        OPTnumber = {},
        OPTseries = {},
        OPTaddress = {},
        OPTmonth = {},
        OPTorganization = {},
