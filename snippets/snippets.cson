    'body': """
      @MastersThesis{${1:author}-${2:year},
        author = {$1},
        title = {$3},
        school = {$4},
        year = {$2},
        OPTkey = {},
        OPTtype = {},
        OPTaddress = {},
        OPTdoi = {},
        OPTmonth = {},
        OPTnote = {},
        OPTannote = {}
      }
    """

  'Misc':
    'prefix': 'misc'
    'body': """
      @Misc{${1:author}-${2:year},
        OPTkey = {},
        OPTauthor = {$1},
        OPTtitle = {},
        OPThowpublished = {},
        OPTmonth = {},
        OPTURL = {},
        OPTyear = {$2},
        OPTnote = {},
        OPTannote = {}
